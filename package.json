{
  "name": "bookstore-e2e-benchmark",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "npm@9.2.0",
  "workspaces": [
    "packages/*",
    "tests/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test:all": "npm run test:playwright && npm run test:cypress && npm run test:selenium",
    "test:playwright": "echo \"(todo)\"",
    "test:cypress": "echo \"(todo)\"",
    "test:selenium": "echo \"(todo)\"",
    "env:up": "docker compose -f apps/bookstore/test-bookstore/docker-test-bookstore/docker-compose.yml up -d && BASE_URL=http://localhost:8080 apps/bookstore/scripts/wait-for-app.sh",
    "env:down": "docker compose -f apps/bookstore/test-bookstore/docker-test-bookstore/docker-compose.yml down",
    "env:reset": "docker compose -f apps/bookstore/test-bookstore/docker-test-bookstore/docker-compose.yml down -v"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "prettier": "^3.0.0"
  }
}
